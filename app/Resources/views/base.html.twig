<!doctype html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

        <title>{% block title %}Rippleffect Symfony Skeleton{% endblock %}</title>

        <!-- css -->
        {% stylesheets 'bundles/core-bundle/css/styles.css' filter="cssrewrite,?yui_css" %}
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}

        {% javascripts '@VendorCoreBundle/Resources/public/js/vendor/jquery.js' filter="?yui_js" %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            {# Any other JS can be loaded in the footer along with the JS router #}
            {% javascripts '@FOSJsRoutingBundle/Resources/public/js/router.js' filter="?yui_js" %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
